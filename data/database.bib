@Article{Weinstein1981BAMS,
  author    = {Weinstein, A.},
  title     = {Symplectic geometry},
  journal   = {Bull. Amer. Math. Soc.},
  year      = {1981},
  volume    = {5},
  pages     = {1--13},
  comment   = {Dans la vision traditionnelle de la mécanique de Newton et particulièrement la mécanique des solides,
la géométrie Euclidienne joue un rôle essentielle: c'est elle qui décrit comment les longueurs sont 
