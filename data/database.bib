  file      = {Vannitsem2002JAS.pdf:Vannitsem2002JAS.pdf:PDF},
  keywords  = {model error, perturbation growth, diffusion, averaging},
  owner     = {pannekou},
  publisher = {American Meteorological Society},
  timestamp = {2013.09.24},
  url       = {http://dx.doi.org/10.1175/1520-0469(2002)059<2594:STDOME>2.0.CO;2},
}

